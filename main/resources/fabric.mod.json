{
  "schemaVersion": 1,
  "id": "els",
  "version": "${version}",
  "name": "Enhanced Loading Screens",
  "description": "A mod that allows you to add and use custom backgrounds in Minecraft Loading screens.",
  "authors": [
    "@MateusMG2010"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/enhanced-loading-screens",
    "sources": "https://github.com/MateusMG2010/Enhanced-Loading-Screens"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/els/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.mgstudios.els.client.EnhancedLoadingScreensClient"
    ]
  },
  "mixins": [
    "els.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}"
  }
}